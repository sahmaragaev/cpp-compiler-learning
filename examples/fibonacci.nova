function void main() {
    int[20] fib;
    int count = 15;
    
    fib[0] = 0;
    fib[1] = 1;
    
    for (int i = 2; i < count; i = i + 1) {
        fib[i] = fib[i - 1] + fib[i - 2];
    }
    
    print("First 15 Fibonacci numbers:");
    for (int i = 0; i < count; i = i + 1) {
        print(fib[i]);
    }
}